    "best_degree = 20\n",
    "best_r_squared = 0.7245\n",
    "sound_degree = 2\n",
    "\n",
    "df['weight_norm'] = df['weight']/df['weight'].mean()\n",
    "#your code here\n",
    "df1=df[['mpg','weight_norm']].dropna()\n",
